! Molecular charge
 $contrl ICHARG=-1 $end

! One of the following
 $guess guess=huckel $end

! Run COSMO on an already optimized geometry
 $contrl runtyp=energy maxit=100 $end
 $COSGMS EPSI=999 rsolv=1.2 delsc=1.2 $end

